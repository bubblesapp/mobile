{
  "headers": [
    {"source": "/service-worker.js", "headers": [{"key": "Cache-Control", "value": "no-cache"}]}
  ],
  "hosting": {
    "public": "web-build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
