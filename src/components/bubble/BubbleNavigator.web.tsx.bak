import React from 'react';
import {Switch} from 'react-router';
import {Redirect, Route} from 'react-router-dom';
import {Bubble} from './Bubble';
import {Invites} from '../invites/Invites';

export const BubbleNavigator: React.FC = () => {
  return (
    <Switch>
      <Redirect exact from="/main/bubble" to="/main/bubble/bubble" />
      <Route path="/main/bubble/bubble" children={<Bubble />} />
      <Route path="/main/bubble/invites" children={<Invites />} />
    </Switch>
  );
};
